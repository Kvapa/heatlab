<%= render "menu" %>

<h2> Důchod </h2>
<p>
<table>
  <tr>
    <th>Osoba</th>
    <th>Odchod do důchodu</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.surname %> <%= user.firstname %></td>
    <td><%= user.contract_end.strftime('%d.%m.%Y') %></td>
  </tr>
<% end %>
</table>
</p>